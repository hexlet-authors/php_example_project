{
    "name": "hexlet/project",
    "type": "project",
    "require": {
        "php": "^7.2.5",
        "laravel/lumen-framework": "^7.0",
        "hexlet/code": "@dev"
    },
    "repositories": {
      "hexlet/code": {
        "type": "path",
        "url": "./code",
        "options": {
          "symlink": true
        }
      }
    },
    "require-dev": {
      "fzaninotto/faker": "^1.9.1",
      "mockery/mockery": "^1.3.1",
      "phpunit/phpunit": "^8.5",
        "codedungeon/phpunit-result-printer": "^0.28.0"
    },
    "autoload": {
      "psr-4": {
        "App\\": "app/"
      }
    },
    "autoload-dev": {
      "classmap": [
        "tests/"
        ]
    }
}
